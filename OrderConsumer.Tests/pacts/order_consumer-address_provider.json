{
  "consumer": {
    "name": "order_consumer"
  },
  "interactions": [
    {
      "description": "A request to delete an address",
      "providerStates": [
        {
          "name": "address ID invalid_address_id is invalid"
        }
      ],
      "request": {
        "method": "DELETE",
        "path": "/address/invalid_address_id"
      },
      "response": {
        "status": 400
      }
    },
    {
      "description": "A request to retrieve an address",
      "providerStates": [
        {
          "name": "address ID invalid_address_id is invalid"
        }
      ],
      "request": {
        "method": "GET",
        "path": "/address/invalid_address_id"
      },
      "response": {
        "status": 400
      }
    },
    {
      "description": "A request to delete an address",
      "providerStates": [
        {
          "name": "address ID invalid_address_id is valid"
        }
      ],
      "request": {
        "method": "DELETE",
        "path": "/address/93edc1a1-5093-4d30-a9c1-da04765553b7"
      },
      "response": {
        "status": 204
      }
    },
    {
      "description": "A request to retrieve an address",
      "providerStates": [
        {
          "name": "address with ID 93edc1a1-5093-4d30-a9c1-da04765553b7 does not exist"
        }
      ],
      "request": {
        "method": "GET",
        "path": "/address/93edc1a1-5093-4d30-a9c1-da04765553b7"
      },
      "response": {
        "status": 404
      }
    },
    {
      "description": "A request to retrieve an address",
      "providerStates": [
        {
          "name": "address with ID 93edc1a1-5093-4d30-a9c1-da04765553b7 exists"
        }
      ],
      "request": {
        "method": "GET",
        "path": "/address/93edc1a1-5093-4d30-a9c1-da04765553b7"
      },
      "response": {
        "body": {
          "AddressType": "billing",
          "City": "Los Angeles",
          "Country": "United States",
          "Id": "93edc1a1-5093-4d30-a9c1-da04765553b7",
          "Number": 123,
          "State": "California",
          "Street": "Main street",
          "ZipCode": 12345
        },
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "matchingRules": {
          "body": {
            "$.AddressType": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.City": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.Country": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.Id": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "regex",
                  "regex": "[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}"
                }
              ]
            },
            "$.Number": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "integer"
                }
              ]
            },
            "$.State": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.Street": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.ZipCode": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "integer"
                }
              ]
            }
          },
          "header": {}
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pactRust": {
      "ffi": "0.4.0",
      "models": "1.0.4"
    },
    "pactSpecification": {
      "version": "3.0.0"
    }
  },
  "provider": {
    "name": "address_provider"
  }
}